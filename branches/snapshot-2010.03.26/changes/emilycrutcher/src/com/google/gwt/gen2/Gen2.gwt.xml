<!-- inherits all top-level modules of then gen2 package.-->
<module>
    <!-- Selection includes Base, Event, CommonWidget -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.google.gwt.gen2.base.Base'/>
    <inherits name='com.google.gwt.gen2.commonevent.CommonEvent'/>
    <inherits name='com.google.gwt.gen2.commonwidget.CommonWidget'/>
    <inherits name='com.google.gwt.gen2.logging.Logging'/>
    <inherits name='com.google.gwt.gen2.picker.Picker'/>
    <inherits name='com.google.gwt.gen2.selection.Selection'/>
    <inherits name='com.google.gwt.gen2.widgetbase.WidgetBase'/>
    <inherits name='com.google.gwt.gen2.complexpanel.ComplexPanel'/>
</module>
